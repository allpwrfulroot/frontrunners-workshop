    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.1.4",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-markdown": "^4.2.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.1.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-module-resolver": "^3.2.0",
